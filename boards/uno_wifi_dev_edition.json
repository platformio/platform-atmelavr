{
  "build": {
    "core": "arduino",
    "extra_flags": "-DARDUINO_AVR_UNO_WIFI_DEV_ED -DAVR_UNO_WIFI_DEV_ED -DESP_CH_UART -DESP_CH_UART_BR=19200",
    "f_cpu": "16000000L",
    "hwids": [
      [
        "0x2A03",
        "0x0057"
      ]
    ],
    "esp_ch_uart_br": 19200,
    "mcu": "atmega328p",
    "variant": "standard"
  },
  "bootloader": {
    "efuse": "0x05",
    "file": "optiboot/optiboot_atmega328.hex",
    "hfuse": "0xDE",
    "lock_bits": "0x0F",
    "lfuse": "0xFF",
    "unlock_bits": "0x3F"
  },
  "debug": {
    "simavr_target": "atmega328p",
    "avr-stub": {
      "speed": 115200
    }
  },
  "frameworks": [
    "arduino"
  ],
  "name": "Arduino Uno Wifi",
  "upload": {
    "maximum_ram_size": 2048,
    "maximum_size": 32256,
    "protocol": "arduino",
    "require_upload_port": true,
    "speed": 115200
  },
  "url": "https://docs.arduino.cc/retired/boards/arduino-uno-wifi",
  "vendor": "Arduino"
}
