{
  "build": {
    "core": "arduino",
    "extra_flags": "-DARDUINO_AVR_BT",
    "f_cpu": "16000000L",
    "mcu": "atmega328p",
    "variant": "eightanaloginputs"
  },
  "bootloader": {
    "efuse": "0xFD", 
    "file": "bt/ATmegaBOOT_168_atmega328_bt.hex", 
    "hfuse": "0xD8", 
    "lock_bits": "0x0F", 
    "lfuse": "0xFF", 
    "unlock_bits": "0x3F"
  },
  "connectivity": [
    "bluetooth"
  ],
  "frameworks": [
    "arduino"
  ],
  "name": "Arduino BT ATmega328",
  "upload": {
    "disable_flushing": true,
    "maximum_ram_size": 2048,
    "maximum_size": 28672,
    "protocol": "arduino",
    "require_upload_port": true,
    "speed": 19200
  },
  "url": "http://arduino.cc/en/main/boards",
  "vendor": "Arduino"
}
